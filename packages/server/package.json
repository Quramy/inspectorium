{
  "name": "@inspectorium/server",
  "version": "0.1.1",
  "description": "inspectorium server",
  "bin": {
    "inspectorium-servser": "built/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [
    "language server protovol"
  ],
  "author": "Quramy",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3",
    "typescript-language-server": "^0.3.2"
  },
  "dependencies": {
    "@inspectorium/schema": "^0.1.1",
    "@types/express": "^4.16.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.9.4",
    "express": "^4.16.3",
    "js-yaml": "^3.12.0",
    "minimist": "^1.2.0",
    "vscode-languageserver-protocol": "^3.12.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(integration-test/.*|(src/.*\\.test))\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "built/.*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
